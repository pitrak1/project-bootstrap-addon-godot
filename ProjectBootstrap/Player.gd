extends Node

var _name
var _host
var _id
var _game

func get_name():
	return _name
	
func set_name(name):
	_name = name

func get_host():
	return _host
	
func set_host(host):
	_host = host

func get_id():
	return _id
	
func set_id(id):
	_id = id

func get_game():
	return _game
	
func set_game(game):
	_game = game
	